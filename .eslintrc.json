{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended"],
  "ignorePatterns": ["src/generated"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["t"],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
